<a href="/admin">Add User</a>

<ul>
    <table >
    <tr>
        <th>ID</th>
        <th>username</th>
        <th>password</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>

    <% for(let i=0;i<users.length;i++){ %>    
        <tr>
            <td><%= users[i].id%></td>
            <td><%= users[i].username%></td>
            <td><%= users[i].password%></td>
            <td><a href="admin/<%= users[i].id%>">Edit</a></td>
            <td><a href="" onClick="del(<%= users[i].id%>)">delete</a></td>
         </tr>
    <%}%>
</table>
  
</ul>



